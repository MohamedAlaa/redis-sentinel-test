include redis-default.conf

daemonize yes
latency-monitor-threshold 10
repl-backlog-size 32mb
repl-disable-tcp-nodelay yes
client-output-buffer-limit slave 2gb 2gb 60
save ""
dir {{ redis.data_path }}
port {{ item.port }}
dbfilename {{ item.port }}.rdb
pidfile {{ item.port }}.pid
logfile {{ item.port }}.log
